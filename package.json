{
  "name": "mocha-mix-jsdom",
  "version": "0.0.1-alpha.1",
  "description": "mocha-mix mocha test jsdom plugin",
  "main": "lib/index.js",
  "scripts": {
    "test": "istanbul cover _mocha $(find ./lib -name *.test.js)",
    "test:simple": "mocha $(find ./lib -name *.test.js)",
    "test:watch": "mocha --watch $(find ./lib -name *.test.js)"
  },
