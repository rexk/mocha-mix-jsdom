    "mocha",
    "test",
    "jsdom",
    "plugin"
  ],
  "engines": {
    "node": ">= 4.0",
    "npm": ">= 2.0"
  },
  "author": "myungsunrex@gmail.com",
  "license": "MIT",
  "dependencies": {
    "jsdom": "^7.2.2"
  },
  "devDependencies": {
    "expect": "^1.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "mocha-mix": "^1.0.0-alpha.0"
  }
}
